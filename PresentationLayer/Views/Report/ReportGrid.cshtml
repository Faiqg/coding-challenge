@model IEnumerable<Data.ViewModel.ReportModel>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DisadvantageFirstYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DisadvantageSecondYear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Comparison)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PlaceName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StateName)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.DisadvantageFirstYear)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.DisadvantageSecondYear)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Comparison)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PlaceName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.StateName)
    </td>
</tr>
}

</table>
